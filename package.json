{
  "name": "mber-rust",
  "type": "module",
  "version": "0.1.3",
  "description": "",
  "main": "lib/index.js",
  "author": "Izel Nakri <contact@izelnakri.com>",
  "license": "MIT",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "npm run package && node-pre-gyp-github publish"
  },
  "dependencies": {
    "mber": "0.8.18",
    "neon-cli": "^0.3.1",
    "node-pre-gyp": "github:amilajack/node-pre-gyp#neon-compat",
    "node-pre-gyp-github": "^1.4.3"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izelnakri/mber-rust.git"
  },
  "files": [
    "native/index.node",
    "lib",
    "native/Cargo.toml",
    "native/Cargo.lock",
    "native/build.rs",
    "native/artifacts.json"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://github.com/izelnakri/mber-rust/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  }
}
