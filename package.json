{
  "name": "mber-rust",
  "version": "0.1.9",
  "description": "",
  "main": "lib/index.js",
  "author": "Izel Nakri <contact@izelnakri.com>",
  "license": "MIT",
  "scripts": {
    "install": "neon build --release",
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node --experimental-modules scripts/build-ember-base.js",
    "build:no-ember-data": "node --experimental-modules scripts/build-ember-base.js --exclude-ember-data",
    "build:test-support": "node --experimental-modules scripts/build-test-support.js",
    "build:memserver": "node --experimental-modules scripts/build-memserver.js",
    "build:fastboot-modules": "node --experimental-modules scripts/transpile-fastboot-modules.js",
    "build": "npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:test-support && npm run build:memserver && npm run build:fastboot-modules"
  },
  "dependencies": {
    "mber": "0.8.18",
    "neon-cli": "^0.3.1"
  },
  "devDependencies": {
    "ansi-colors": "^4.1.1",
    "ember-source": "3.13.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "fastboot": "^2.0.3",
    "fs-extra": "^8.1.0",
    "loader.js": "^4.7.0",
    "memserver": "^1.2.10",
    "sass": "^1.23.0",
    "terser": "^4.3.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izelnakri/mber-rust.git"
  },
  "files": [
    "lib",
    "native/Cargo.toml",
    "native/Cargo.lock",
    "native/src",
    "native/build.rs",
    "native/artifacts.json"
  ]
}
